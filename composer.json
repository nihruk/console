{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/yaml": "6.2.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18.4",
        "slevomat/coding-standard": "^8.12.1",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2",
        "vimeo/psalm": "^5.12"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/src"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "@test-psalm",
            "@test-phpstan",
            "@test-phpcs",
            "@test-phpunit"
        ],
        "test-psalm": "./vendor/bin/psalm --no-cache",
        "test-phpstan": "./vendor/bin/phpstan analyse",
        "test-phpcs": "./vendor/bin/phpcs",
        "test-phpunit": [
            "@test-phpunit-unit",
            "@test-phpunit-web",
            "@test-phpunit-integration"
        ],
        "test-phpunit-unit": "./vendor/bin/phpunit --testsuite unit",
        "test-phpunit-web": "./vendor/bin/phpunit --testsuite web",
        "test-phpunit-integration": "./vendor/bin/phpunit --testsuite integration",
        "fix": [
            "@fix-phpcbf"
        ],
        "fix-phpcbf": "./vendor/bin/phpcbf"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    }
}
